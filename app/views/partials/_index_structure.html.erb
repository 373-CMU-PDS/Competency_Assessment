<h1><%= model_name.pluralize.capitalize %></h1>

<%= render "#{primary}" %>

<% unless secondary.nil? %>
  <br>
  <%= render "#{secondary}" %>
<% end %>

<br>
<% if ["resource","question"].exclude? model_name %>

  <%= link_to(eval("new_#{model_name}_path")) do %>
    Add a New <%= model_name.capitalize %>
  <% end %>
<% end %>
<br><br>
